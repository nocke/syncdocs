{
  // extensions you consider either not to “documents”
  // and/or too big (like movies) or otherwise not desirable (i.e. executables)
  "excludedExtensions": [
      "exe",
      "cr2",
      "crw",
      "dng",
      "raw",
      "mp3",
      "ogg",
      "wav",
      "mp4",
      "mov",
      "flv",
      "f4v",
      "swf",
      "wmv",
      "wma",
      "avi",
      "mkv"
  ],
  // max individual file size in MB
  "MAX_FILE_SIZE_MB": 30,
  // NOT defined here, but required in each `.synclocal.json`:
  // • machineName (according to uname -n, as a form of assurance)
  // • shareRepo (like "/share/docs" or simlar mount, point to the central share repo)
}
